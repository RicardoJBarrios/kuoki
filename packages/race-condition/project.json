{
  "root": "packages/race-condition",
  "sourceRoot": "packages/race-condition/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/race-condition",
        "main": "packages/race-condition/src/index.ts",
        "tsConfig": "packages/race-condition/tsconfig.lib.json",
        "assets": ["packages/race-condition/*.md"]
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/race-condition/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/race-condition"],
      "options": {
        "jestConfig": "packages/race-condition/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "docs": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "npx typedoc --options packages/race-condition/typedoc.json",
          "nx test race-condition --code-coverage --skip-nx-cache"
        ],
        "parallel": false
      }
    },
    "version": {
      "executor": "@jscutlery/semver:version",
      "options": {
        "commitMessageFormat": "chore(${projectName}): release version ${version}",
        "tagPrefix": "${projectName}-v",
        "noVerify": true,
        "push": true,
        "postTargets": ["race-condition:github"]
      }
    },
    "github": {
      "executor": "@jscutlery/semver:github",
      "options": {
        "tag": "${tag}",
        "notes": "${notes}"
      }
    }
  },
  "tags": []
}
