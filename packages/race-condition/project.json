{
  "root": "packages/race-condition",
  "sourceRoot": "packages/race-condition/src",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/race-condition",
        "main": "packages/race-condition/src/index.ts",
        "tsConfig": "packages/race-condition/tsconfig.lib.json",
        "assets": ["packages/race-condition/*.md"]
      }
    },
    "publish": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "command": "node tools/scripts/publish.mjs race-condition {args.ver} {args.tag}",
        "cwd": "dist/packages/race-condition"
      },
      "dependsOn": [
        {
          "projects": "self",
          "target": "build"
        }
      ]
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/race-condition/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/packages/race-condition"],
      "options": {
        "jestConfig": "packages/race-condition/jest.config.ts",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
