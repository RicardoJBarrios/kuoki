{
  "name": "kuoki",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Libraries to help with JavasScript frontend development",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "audit:deploy": "yarn audit --groups dependencies",
    "audit:fix": "yarn-audit-fix",
    "commit": "cz",
    "format:all": "nx format:write --all",
    "lint:all": "nx run-many --all --target=lint",
    "nx": "nx",
    "prepare": "husky install",
    "update:nx": "nx migrate latest && yarn && (test -f migrations.json && (nx migrate --run-migrations && rm migrations.json) || 2>/dev/null)",
    "postinstall": "ngcc --properties es2015 browser module main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "13.3.2",
    "@angular/common": "13.3.2",
    "@angular/compiler": "13.3.2",
    "@angular/core": "13.3.2",
    "@angular/forms": "13.3.2",
    "@angular/platform-browser": "13.3.2",
    "@angular/platform-browser-dynamic": "13.3.2",
    "@angular/router": "13.3.2",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "13.3.2",
    "@angular-eslint/eslint-plugin": "~13.0.1",
    "@angular-eslint/eslint-plugin-template": "~13.0.1",
    "@angular-eslint/template-parser": "~13.0.1",
    "@angular/cli": "13.3.2",
    "@angular/compiler-cli": "13.3.2",
    "@angular/language-service": "13.3.2",
    "@commitlint/cli": "^17.0.0",
    "@commitlint/config-conventional": "^17.0.0",
    "@jscutlery/semver": "^2.23.3",
    "@ngneat/spectator": "^10.0.1",
    "@nrwl/angular": "14.1.9",
    "@nrwl/cli": "14.1.9",
    "@nrwl/eslint-plugin-nx": "14.1.9",
    "@nrwl/jest": "14.1.9",
    "@nrwl/js": "14.1.9",
    "@nrwl/linter": "14.1.9",
    "@nrwl/nx-cloud": "14.0.5",
    "@nrwl/web": "^14.1.9",
    "@nrwl/workspace": "14.1.9",
    "@sinonjs/fake-timers": "^9.1.2",
    "@types/jest": "27.4.1",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "14.14.33",
    "@types/sinonjs__fake-timers": "^8.1.2",
    "@typescript-eslint/eslint-plugin": "5.18.0",
    "@typescript-eslint/parser": "5.18.0",
    "babel-preset-minify": "^0.5.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^8.0.1",
    "jest": "27.5.1",
    "jest-create-mock-instance": "^2.0.0",
    "jest-extended": "^2.0.0",
    "jest-fetch-mock": "^3.0.3",
    "jest-html-reporters": "^3.0.8",
    "jest-preset-angular": "11.1.1",
    "lint-staged": "^12.4.2",
    "lodash": "^4.17.21",
    "ng-packagr": "13.3.0",
    "nx": "14.1.9",
    "postcss": "^8.4.5",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "prettier": "2.5.1",
    "rxjs-marbles": "^7.0.1",
    "ts-essentials": "^9.1.2",
    "ts-jest": "27.1.4",
    "ts-node": "9.1.1",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.1",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "4.6.3",
    "yarn-audit-fix": "^9.3.1"
  },
  "resolutions": {
    "minimist": ">=1.2.6"
  },
  "lint-staged": {
    "*": [
      "nx format:write --files"
    ],
    "*.{js,jsx,ts,tsx}": [
      "nx affected:lint --fix --files"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "kuoki",
          "environment",
          "environment-angular",
          "race-condition"
        ]
      ]
    }
  }
}
