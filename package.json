    "@angular/compiler-cli": "15.2.6",
    "@angular/language-service": "15.2.6",
    "@babel/core": "^7.0.0",
    "@commitlint/cli": "~17.5.1",
    "@commitlint/config-conventional": "~17.4.4",
    "@commitlint/cz-commitlint": "~17.5.0",
