{
  "name": "kuoki",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Libraries to help with JavasScript frontend development",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "audit:deploy": "yarn audit --groups dependencies",
    "audit:fix": "yarn-audit-fix",
    "commit": "cz",
    "format:all": "nx format:write --all",
    "build:all": "nx run-many --all --target=build",
    "lint:all": "nx run-many --all --target=lint",
    "test:all": "nx run-many --all --target=test",
    "nx": "nx",
    "prepare": "husky install",
    "update:nx": "nx migrate latest && yarn && (test -f migrations.json && (nx migrate --run-migrations && rm migrations.json) || 2>/dev/null)",
    "postinstall": "ngcc --properties es2020 browser module main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "15.0.4",
    "@angular/common": "15.0.4",
    "@angular/compiler": "15.0.4",
    "@angular/core": "15.0.4",
    "@angular/forms": "15.0.4",
    "@angular/platform-browser": "15.0.4",
    "@angular/platform-browser-dynamic": "15.0.4",
    "@angular/router": "15.0.4",
    "lodash-es": "^4.17.21",
    "rxjs": "~7.5.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "15.0.4",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "~15.0.0",
    "@angular/compiler-cli": "15.0.4",
    "@angular/language-service": "15.0.4",
    "@commitlint/cli": "~17.0.2",
    "@commitlint/config-conventional": "~17.0.2",
    "@jscutlery/semver": "~2.25.2",
    "@ngneat/spectator": "~11.0.2",
    "@nrwl/angular": "15.4.2",
    "@nrwl/cli": "15.4.2",
    "@nrwl/eslint-plugin-nx": "15.4.2",
    "@nrwl/jest": "15.4.2",
    "@nrwl/js": "15.4.2",
    "@nrwl/linter": "15.4.2",
    "@nrwl/nx-cloud": "15.0.2",
    "@nrwl/web": "15.4.2",
    "@nrwl/workspace": "15.4.2",
    "@sinonjs/fake-timers": "~9.1.2",
    "@types/jest": "28.1.8",
    "@types/lodash-es": "~4.17.6",
    "@types/node": "18.7.1",
    "@types/sinonjs__fake-timers": "~8.1.2",
    "@typescript-eslint/eslint-plugin": "5.47.1",
    "@typescript-eslint/parser": "5.47.1",
    "babel-preset-minify": "~0.5.2",
    "commitizen": "~4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "~8.0.1",
    "jest": "28.1.3",
    "jest-create-mock-instance": "~2.0.0",
    "jest-environment-jsdom": "28.1.1",
    "jest-extended": "~2.0.0",
    "jest-fetch-mock": "~3.0.3",
    "jest-html-reporters": "~3.0.9",
    "jest-preset-angular": "12.2.3",
    "lint-staged": "~13.0.3",
    "lodash": "~4.17.21",
    "ng-packagr": "15.0.3",
    "nx": "15.4.2",
    "postcss": "^8.4.5",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "prettier": "2.7.1",
    "rxjs-marbles": "~7.0.1",
    "ts-essentials": "~9.1.2",
    "ts-jest": "28.0.8",
    "ts-node": "10.9.1",
    "typedoc": "~0.23.0",
    "typedoc-plugin-markdown": "~3.14.0",
    "typedoc-plugin-missing-exports": "~1.0.0",
    "typescript": "4.8.4",
    "yarn-audit-fix": "~9.3.2"
  },
  "resolutions": {
    "minimist": ">=1.2.6"
  },
  "lint-staged": {
    "*": [
      "nx format:write --files"
    ],
    "*.{js,jsx,ts,tsx}": [
      "nx affected:lint --fix --files"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "kuoki",
          "anymap",
          "environment",
          "environment-angular",
          "race-condition"
        ]
      ]
    }
  }
}
