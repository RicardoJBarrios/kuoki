{
  "name": "kuoki",
  "version": "0.0.0",
  "license": "MIT",
  "description": "Libraries to help with frontend development",
  "engines": {
    "node": "^12.22.0 || ^14.17.0 || >=16.0.0",
    "yarn": ">=1.22.4 <2",
    "npm": "Please use yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "commit": "cz",
    "format:all": "nx format:write --all",
    "update:nx": "nx migrate latest && yarn && (test -f migrations.json && (nx migrate --run-migrations && rm migrations.json) || 2>/dev/null)"
  },
  "private": true,
  "dependencies": {
    "lodash-es": "^4.17.21",
    "rxjs": "~7.4.0",
    "tslib": "^2.0.0",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@jscutlery/semver": "^2.17.5",
    "@nrwl/cli": "13.8.3",
    "@nrwl/eslint-plugin-nx": "13.8.3",
    "@nrwl/jest": "13.8.3",
    "@nrwl/js": "13.8.3",
    "@nrwl/linter": "13.8.3",
    "@nrwl/nx-cloud": "13.1.4",
    "@nrwl/tao": "13.8.3",
    "@nrwl/workspace": "13.8.3",
    "@sinonjs/fake-timers": "^9.1.0",
    "@types/jest": "27.0.2",
    "@types/lodash-es": "^4.17.5",
    "@types/node": "14.14.33",
    "@types/sinonjs__fake-timers": "^8.1.1",
    "@types/uuid": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "5.10.2",
    "@typescript-eslint/parser": "5.10.2",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.1.0",
    "husky": "^7.0.4",
    "jest": "27.2.3",
    "jest-create-mock-instance": "^2.0.0",
    "jest-extended": "^1.2.0",
    "jest-html-reporters": "^3.0.4",
    "lodash": "^4.17.21",
    "prettier": "2.5.1",
    "rxjs-marbles": "^7.0.1",
    "ts-essentials": "^9.1.0",
    "ts-jest": "27.0.5",
    "typedoc": "^0.22.10",
    "typedoc-plugin-markdown": "^3.11.8",
    "typedoc-plugin-missing-exports": "^0.22.6",
    "typescript": "~4.4.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ],
    "rules": {
      "scope-enum": [
        2,
        "always",
        [
          "kuoki",
          "environment"
        ]
      ]
    }
  }
}

